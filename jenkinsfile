pipeline {
    agent any
        stages {
        stage('Checkout SCM') {
            steps {
                checkout scm
            }
        }
        stage('Check SF version'){
            agent{
                docker{
                    image 'docker pull salesforce/cli:latest-rc-full'
                }
            }
        steps{
            script{
                sh 'sf version'
            }
        }
        }
        }
}